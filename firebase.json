{
  "flutter": {
    "platforms": {
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "viridis-bdd08",
          "configurations": {
            "web": "1:713890148209:web:d722627ab16bbe21c010d0"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "headers": [
      {
        "source": "**/*.mp4",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Content-Type",
            "value": "video/mp4"
          },
          {
            "key": "cache-control",
            "value": "public, max-age=31536000"
          },
          {
            "key": "x-content-type-options",
            "value": "nosniff"
          }
        ]
      }
    ]
  }
}
